{
  "name": "boilerplate-website",
  "version": "0.0.0",
  "scripts": {
    "build": "next build && next export -o build-next-static",
    "dev": "next dev --port 3101",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "tsc": "tsc -p tsconfig.json",
    "test": "jest --verbose",
    "test-watch": "jest --verbose --watch",
    "test-coverage": "jest --coverage --verbose",
    "storybook": "start-storybook -p 6101 --quiet",
    "storybook-build": "build-storybook -o build-storybook-static",
    "update-dependencies": "npx npm-check-updates -u"
  },
  "dependencies": {
    "config-mui": "*",
    "ui": "*",
    "utils-mui": "*",
    "utils-version": "*"
  },
  "devDependencies": {
    "config-storybook": "*",
    "config-tsconfig": "*",
    "eslint-config-base": "*",
    "utils-test": "*"
  }
}
